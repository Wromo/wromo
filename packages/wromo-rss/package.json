{
  "name": "@wromojs/rss",
  "description": "Add RSS feeds to your Wromo projects",
  "version": "0.2.0",
  "type": "module",
  "types": "./dist/index.d.ts",
  "author": "withwromo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/withwromo/wromo.git",
    "directory": "packages/wromo-rss"
  },
  "bugs": "https://github.com/withwromo/wromo/issues",
  "homepage": "https://wromo.build",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "scripts": {
