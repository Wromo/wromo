    "wromo": "workspace:*",
    "wromo-scripts": "workspace:*",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "mocha": "^9.2.2"
  },
  "dependencies": {
    "fast-xml-parser": "^4.0.8"
  }
}
